    "@babel/polyfill": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "@types/chance": "1.0.0",
    "@types/color": "3.0.0",
    "@types/dateformat": "1.0.1",
    "@types/file-saver": "1.3.0",
    "@types/handlebars": "4.0.36",
    "@types/humps": "1.1.2",
    "@types/jquery": "3.3.1",
    "@types/node": "9.6.2",
    "@types/pixi.js": "4.7.2",
    "@types/query-string": "5.1.0",
    "@types/screenfull": "3.3.1",
    "@types/storejs": "2.0.2",
    "@types/uuid": "3.4.3",
    "@types/webpack-env": "1.13.5",
    "babel-loader": "8.0.0-beta.2",
    "brfs": "1.5.0",
    "chance": "1.0.13",
    "chess.js": "0.10.2",
    "color": "3.0.0",
    "css-loader": "0.28.11",
    "dateformat": "3.0.3",
    "eases": "1.0.8",
    "file-loader": "1.1.11",
    "file-saver": "1.3.8",
    "font-awesome": "4.7.0",
    "font-awesome-sass-loader": "2.0.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "humps": "2.0.1",
    "jquery": "3.3.1",
    "less": "2.3.1",
    "node-sass": "4.8.3",
    "normalize.css": "8.0.0",
    "pixi.js": "4.7.3",
    "query-string": "6.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "screenfull": "3.3.2",
    "store": "2.0.12",
    "style-loader": "0.20.3",
    "transform-loader": "0.2.4",
    "ts-loader": "4.1.0",
    "ts-node": "5.0.1",
    "ts-typed-events": "1.1.1",
    "typescript": "2.8.1",
    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "yarn": "^1.5.1"
  },
  "devDependencies": {
    "@types/webpack": "4.1.3",
    "http-server": "0.11.1",
    "husky": "0.14.3",
    "npm-check-updates": "2.14.1",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "5.1.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.2"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:raw": "webpack --progress --colors",
    "build:dev": "npm run build:raw -- --mode development",
    "build:prod": "npm run build:raw -- --mode production",
    "clean": "npm run clean:build && npm run clean:docs",
    "clean:docs": "rimraf docs/",
    "clean:build": "rimraf dist/",
    "docs": "typedoc --tsconfig ./tsconfig.json --out docs --module commonjs",
    "check-updates": "npm-check-updates -u",
    "lint": "tslint --project ./tsconfig.json --config tslint.json",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix --force",
    "server": "http-server dist/",
    "start": "npm run watch",
    "test": "npm run lint && npm run build",
    "watch": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --content-base dist/ --mode development",
    "precommit": "npm run test",
    "postmerge": "npm install",
    "postrewrite": "npm install"
  }
}
