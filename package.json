    "file-loader": "2.0.0",
    "file-saver": "1.3.8",
    "font-awesome": "4.7.0",
    "font-awesome-sass-loader": "2.0.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "handlebars": "4.0.12",
