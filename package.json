    "@types/storejs": "2.0.3",
    "@types/uuid": "3.4.4",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "8.0.4",
    "cadre-ts-utils": "github:siggame/Cadre-TS-Utils#eb27dff0bd93f1979fc28ff1ce8a10ab74145d7d",
    "chance": "1.0.16",
