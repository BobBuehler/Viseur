{
  "name": "Viseur",
  "version": "1.0.0",
  "description": "Visualizer for the Cadre AI game framework",
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Viseur"
  },
  "scripts": {
    "visualizer": "./node_modules/.bin/webpack-dev-server --progress --colors --host 0.0.0.0",
    "bundle": "./node_modules/.bin/webpack ./main.js",
    "lint": "./node_modules/.bin/eslint --cache ./",
    "lintfix": "./node_modules/.bin/eslint --fix ./"
  },
  "private": true,
  "dependencies": {
    "cadre-js-extensions": "siggame/Cadre-js-extensions",
    "chess.js": "0.10.2",
    "classe": "1.0.0",
    "color": "0.11.1",
    "dateformat": "1.0.12",
    "eases": "1.0.8",
    "filesaverjs": "0.2.2",
    "humps": "1.1.0",
    "jquery": "2.2.3",
    "pixi.js": "3.0.11",
    "query-string": "4.1.0",
    "store": "1.3.20"
  },
  "devDependencies": {
    "brfs": "^1.4.3 ",
    "css-loader": "^0.23.1",
    "eslint": "^3.7.1",
    "file-loader": "^0.9.0",
    "font-awesome-webpack": "^0.0.4",
    "ghooks": "^1.3.2",
    "handlebars-loader": "^1.3.0",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "transform-loader": "^0.2.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.15.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "./node_modules/.bin/eslint --cache ./",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "license": "MIT"
}
