{
  "name": "viseur",
  "version": "2.3.0",
  "description": "Visualizer for the Cadre AI game framework",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Viseur"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@cadre/ts-utils": "1.0.5",
    "@types/chess.js": "0.10.0",
    "@types/color": "3.0.0",
    "@types/dateformat": "1.0.1",
    "@types/eases": "1.0.0",
    "@types/file-saver": "1.3.1",
    "@types/handlebars": "4.0.39",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jquery": "3.3.22",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.1",
    "@types/number-to-words": "1.2.0",
    "@types/pixi.js": "4.8.2",
    "@types/query-string": "6.1.1",
    "@types/screenfull": "3.3.2",
    "@types/seedrandom": "2.4.27",
    "@types/storejs": "2.0.3",
    "@types/uuid": "3.4.4",
    "@types/webpack-env": "1.13.6",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "3.3.4",
    "chess.js": "0.10.2",
    "color": "3.1.0",
    "css-loader": "1.0.1",
    "dateformat": "3.0.3",
    "eases": "1.0.8",
    "file-loader": "2.0.0",
    "file-saver": "1.3.8",
    "font-awesome": "4.7.0",
    "font-awesome-sass-loader": "2.0.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "handlebars": "4.0.12",
    "handlebars-loader": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jquery": "3.3.1",
    "less": "3.8.1",
    "lodash": "4.17.11",
    "node-sass": "4.9.4",
    "normalize.css": "8.0.0",
    "number-to-words": "1.2.4",
    "pixi.js": "4.8.2",
    "query-string": "6.2.0",
    "raw-loader": "0.5.1",
    "sass-loader": "7.1.0",
    "screenfull": "3.3.3",
    "seedrandom": "2.4.4",
    "store": "2.0.12",
    "style-loader": "0.23.1",
    "transform-loader": "0.2.4",
    "ts-loader": "5.3.0",
    "ts-node": "7.0.1",
    "ts-typed-events": "1.1.1",
    "typescript": "3.1.4",
    "url-loader": "1.1.2",
    "webpack": "4.23.1"
  },
  "devDependencies": {
    "@types/webpack": "4.4.17",
    "http-server": "0.11.1",
    "husky": "1.1.3",
    "shx": "0.3.2",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "webpack-cli": "3.1.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dev-server": "3.1.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:raw": "webpack --progress --colors",
    "build:dev": "npm run build:raw -- --mode development",
    "build:prod": "npm run build:raw -- --mode production",
    "build:stats": "webpack --profile --json > webpack-stats.json",
    "clean": "npm run clean:build && npm run clean:docs && npm run clean:stats",
    "clean:docs": "shx rm -rf docs/",
    "clean:build": "shx rm -rf dist/",
    "clean:stats": "shx rm -rf webpack-stats.json",
    "analyze:stats": "webpack-bundle-analyzer webpack-stats.json",
    "analyze": "npm run build:stats && npm run analyze:stats",
    "docs": "typedoc --tsconfig ./tsconfig.json --out docs --module commonjs",
    "lint": "tslint --project ./tsconfig.json --config tslint.json",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix --force",
    "js": "http-server dist/",
    "watch": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --content-base dist/ --mode development",
    "start": "npm run watch",
    "test": "npm run lint && npm run build"
  }
}
